{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/style/:path*",
        "destination": "/public/style/:path*"
      },
      {
        "source": "/script.js",
        "destination": "/public/script.js"
      },
      {
        "source": "/(.*)",
        "destination": "/server.js"
      }
    ]
}